#include <iostream>
#include <vector>
using namespace std;

int main() {
    int n, q;
    cin >> n >> q;   // n = number of arrays, q = number of queries

    vector<vector<int>> arr(n);  // Create vector of vectors

    // Input each variable-length array
    for (int i = 0; i < n; i++) {
        int k;
        cin >> k;  // number of elements in this array
        arr[i].resize(k);
        for (int j = 0; j < k; j++) {
            cin >> arr[i][j];
        }
    }

    // Answer queries
    for (int i = 0; i < q; i++) {
        int x, y;
        cin >> x >> y;
        cout << arr[x][y] << endl;
    }

    return 0;
}
